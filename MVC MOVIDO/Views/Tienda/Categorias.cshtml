@model IEnumerable<ProyectoMoya.Models.Category>

@{
    ViewBag.Title = "Categories";
}

<div class="container">
    <div class="row">
        <div class="col-sm-4 col-sm-offset-1">
            <h2>@ViewBag.Title</h2>
            <ul>
                @foreach (var item in Model)
            {
                    <li><h4>@item.Nombre</h4></li>
                }
            </ul>
        </div>
    </div>
</div>